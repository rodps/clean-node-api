{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx prisma migrate deploy && node dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "test": "npm run migrate:test && cross-env NODE_ENV=test jest --runInBand",
    "test:staged": "npm test -- --passWithNoTests --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:integration": "npm test ./tests/integration",
    "test:unit": "jest --noStackTrace ./src",
    "db:up": "docker-compose -f docker-compose.dev.yml up -d",
    "db:down": "docker-compose -f docker-compose.dev.yml down",
    "migrate": "dotenv -e .env.dev -- npx prisma migrate dev",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "dev": "cross-env NODE_ENV=dev nodemon ./src/main/server.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.3",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.42",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^13.0.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.4",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fp-ts": "^2.12.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "swagger-ui-express": "^4.5.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
