{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand",
    "test:staged": "npm test -- --findRelatedTests",
    "test:unit": "npm test -- --noStackTrace ./src",
    "test:ci": "npm test -- --coverage",
    "test:integration": "dotenv -e .env.test -- npx prisma migrate deploy && npm test ./tests/integration",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "migrate": "dotenv -e .env.dev -- npx prisma migrate dev",
    "dev": "dotenv -e .env.dev -- npx ts-node ./src/main/server.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.3",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "faker": "^5.5.3",
    "fp-ts": "^2.12.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^13.0.1",
    "prisma": "^4.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.4",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0"
  }
}
